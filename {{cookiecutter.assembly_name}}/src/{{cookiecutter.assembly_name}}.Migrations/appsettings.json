{
  "{{cookiecutter.database}}": {
    {%- if cookiecutter.database == "Mongo" -%}
    "Database": {{cookiecutter.assembly_name}},
    {% endif %}
    "ConnectionString": "__SECRET_PLACEHOLDER__"
  },
  "Runner": {
    "HardReset": false
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Couchbase": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  }
}
