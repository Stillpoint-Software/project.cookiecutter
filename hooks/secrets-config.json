{
  "oauth": {
    "condition": "include_oauth",
    "secrets": {
      "OAuth:AppName": "oauth_app_name",
      "OAuth:Audience": "oauth_audience",
      "OAuth:Domain": "oauth_domain",
      "OAuth:Api:Audience": "oauth_api_audience",
      "OAuth:Api:ClientSecret": "oauth_api_client_secret",
      "OAuth:Api:ClientId": "oauth_api_client_id",
      "OAuth:Swagger:ClientId": "oauth_swagger_client_id",
      "OAuth:Swagger:ClientSecret": "oauth_swagger_client_secret"
    }
  },
  "azure_application_insights": {
    "condition": "include_azure_application_insights",
    "secrets": {
      "Parameters:AppInsightsName": "azure_application_insights_name",
      "Parameters:AppInsightsConnection": "azure_application_insights_connection"
    }
  },
  "azure_key_vault": {
    "condition": [
      "include_azure_key_vault",
      "use_existing_azure_key_vault"
    ],
    "secrets": {
      "Parameters:KeyVaultName": "azure_key_vault_name",
      "Parameters:KeyVaultConnection": "azure_key_vault_connection"
    }
  },
  "azure_storage_existing": {
    "condition": [
      "include_azure_storage",
      "use_existing_azure_storage"
    ],
    "secrets": {
      "Parameters:StorageAccountName": "azure_storage_account_name",
      "Parameters:StorageConnection": "azure_storage_connection"
    }
  },
  "azure_storage_blob": {
    "condition": "include_azure_storage",
    "secrets": {
      "Parameters:BlobName": "azure_storage_blob_name"
    }
  },
  "azure_common": {
    "condition_any": [
      [
        "include_azure_key_vault",
        "use_existing_azure_key_vault"
      ],
      [
        "include_azure_application_insights"
      ],
      [
        "include_azure_storage",
        "use_existing_azure_storage"
      ]
    ],
    "secrets": {
      "Azure:TenantId": "azure_tenant_id",
      "Azure:SubscriptionId": "azure_subscription_id",
      "Azure:Location": "azure_location",
      "Parameters:ResourceGroup": "azure_resource_group"
    }
  }
}